{
"manifest_version": 2,
    "name": "The Truth about Coding",
    "description": "Seeks and replaces 'coding' with a more accurate description.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }

}